{
	"name": "description-to-co-authors",
	"version": "0.3.0",
	"description": "Parses co-authors from Git/GitHub-style commit messages and issue bodies. ✍️",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoshuaKGoldberg/description-to-co-authors"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.0",
		"@eslint/js": "9.20.0",
		"@release-it/conventional-changelog": "10.0.0",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/eslint__js": "8.42.3",
		"@types/node": "22.13.0",
		"@vitest/coverage-v8": "3.0.0",
		"console-fail-test": "0.5.0",
		"cspell": "8.17.0",
		"eslint": "9.20.0",
		"eslint-plugin-jsdoc": "50.6.0",
		"eslint-plugin-jsonc": "2.19.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.15.0",
		"eslint-plugin-package-json": "0.26.0",
		"eslint-plugin-perfectionist": "3.9.0",
		"eslint-plugin-regexp": "2.7.0",
		"eslint-plugin-vitest": "0.5.4",
		"eslint-plugin-yml": "1.16.0",
		"husky": "9.1.4",
		"jsonc-eslint-parser": "2.4.0",
		"knip": "5.43.1",
		"lint-staged": "15.4.0",
		"markdownlint": "0.37.0",
		"markdownlint-cli": "0.44.0",
		"npm-package-json-lint": "8.0.0",
		"prettier": "3.5.0",
		"prettier-plugin-curly": "0.3.0",
		"prettier-plugin-packagejson": "2.5.1",
		"prettier-plugin-sh": "0.14.0",
		"release-it": "18.1.0",
		"sentences-per-line": "0.3.0",
		"should-semantic-release": "0.3.0",
		"tsup": "8.3.0",
		"typescript": "5.7.2",
		"typescript-eslint": "8.24.0",
		"vitest": "3.0.0"
	},
	"packageManager": "pnpm@10.3.0",
	"engines": {
		"node": ">=18.3.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
